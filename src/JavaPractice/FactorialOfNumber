
import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
    System.out.println("Enter the number..");
        int i=1,fact=1;
    int a=sc.nextInt();
    
    while(i<=a){
        fact=fact*i;
        i++;
    }
            System.out.println("Factorial of" + " " +a +" " + "is" + " " + fact);
    }
}
