package Live_Test_Java;

public class Multiplication_table {
    public static void main(String[] args) {
        int a=19;
        System.out.println("Multiplication table of" +" " + a);
        for(int i=1;i<=20;i++) {
            for (int j=1;j<=10;j++){
                System.out.print(i + "X" + j + "=" + i * j +" ");
            }
            System.out.println(" ");
        }
    }
}
